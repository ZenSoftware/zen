    "class-validator": "^0.12.2",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.15.0",
    "dotenv": "6.2.0",
    "graphql": "^15.3.0",
    "helmet": "^3.23.3",
    "lodash-es": "^4.17.14",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.5"
