{
  "name": "zen",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "main": "apps/electron/src/app.js",
  "scripts": {
    "start": "nx serve portal",
    "start:api": "concurrently \"nx serve api\" \"gulp handlebars:copy\" \"gulp handlebars:watch\"",
    "start:electron": "ng build --project electron --configuration development && electron .",
    "start:e2e": "nx e2e portal-e2e --watch",
    "apollo:gen": "graphql-codegen -c apollo-angular.yml && graphql-codegen -c apollo-possible-types.yml",
    "gql:gen": "prisma generate && gulp gql:gen",
    "prisma:migrate": "prisma migrate dev && gulp gql:gen",
    "prisma:create-only": "prisma migrate dev --create-only",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prod:apps": "nx build portal --prod",
    "prod:api": "nx build api --prod && npm run dockerize:api",
    "dockerize:api": "gulp create-deploy-package && docker build -f deploy/api/Dockerfile -t zen-api .",
    "docker:run-api": "docker run -d --name zen-api -p 7080:7080 zen-api",
    "docs": "ng compodoc api",
    "test": "nx run-many --all --target=test --parallel",
    "lint": "nx run-many --all --target=lint --parallel",
    "format": "nx format:write --all",
    "build": "nx build",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@graphql-tools/schema": "^8.3.1",
    "@nest-modules/mailer": "^1.3.22",
    "@nestjs/apollo": "^10.0.4",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.0.4",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/throttler": "^3.0.0",
    "@paljs/plugins": "4.1.0",
    "@prisma/client": "~3.15.0",
    "apollo-server-core": "~3.10.0",
    "apollo-server-express": "~3.10.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "date-fns": "^2.17.0",
    "graphql": "^15.0.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-upload": "^13.0.0",
    "helmet": "^5.0.0",
    "lodash-es": "^4.17.20",
    "nodemailer": "^6.4.18",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "prisma": "~3.15.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.5.0",
    "ts-morph": "^15.1.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^14.0.0",
    "@angular-devkit/build-angular": "~14.1.0",
    "@angular-eslint/eslint-plugin": "~14.0.0",
    "@angular-eslint/eslint-plugin-template": "~14.0.0",
    "@angular-eslint/template-parser": "~14.0.0",
    "@angular/animations": "14.1.1",
    "@angular/cdk": "14.1.1",
    "@angular/cli": "~14.1.0",
    "@angular/common": "14.1.1",
    "@angular/compiler": "14.1.1",
    "@angular/compiler-cli": "14.1.1",
    "@angular/core": "14.1.1",
    "@angular/forms": "14.1.1",
    "@angular/language-service": "14.1.1",
    "@angular/localize": "14.1.1",
    "@angular/material": "14.1.1",
    "@angular/platform-browser": "14.1.1",
    "@angular/platform-browser-dynamic": "14.1.1",
    "@angular/router": "14.1.1",
    "@apollo/client": "^3.2.0",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/fragment-matcher": "^3.2.0",
    "@graphql-codegen/typescript": "^2.4.0",
    "@graphql-codegen/typescript-apollo-angular": "^3.3.0",
    "@graphql-codegen/typescript-operations": "^2.2.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/angular": "14.5.4",
    "@nrwl/cli": "14.5.4",
    "@nrwl/cypress": "14.5.4",
    "@nrwl/eslint-plugin-nx": "14.5.4",
    "@nrwl/jest": "14.5.4",
    "@nrwl/linter": "14.5.4",
    "@nrwl/nest": "14.5.4",
    "@nrwl/node": "14.5.4",
    "@nrwl/workspace": "14.5.4",
    "@paljs/cli": "4.1.0",
    "@types/apollo-upload-client": "^17.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/jest": "27.4.1",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "18.0.0",
    "@types/passport": "^1.0.6",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "5.24.0",
    "@typescript-eslint/parser": "5.24.0",
    "apollo-angular": "^3.0.0",
    "apollo-upload-client": "^17.0.0",
    "bootstrap": "5.2.0",
    "bootswatch": "5.2.0",
    "codelyzer": "^6.0.1",
    "concurrently": "^7.0.0",
    "cypress": "^10.2.0",
    "cz-customizable": "^6.3.0",
    "del": "^6.0.0",
    "dotenv": "^16.0.0",
    "electron": "^19.0.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "gulp": "^4.0.2",
    "gulp-flatten": "^0.4.0",
    "gulpclass": "^0.2.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "27.5.1",
    "jest-preset-angular": "11.1.2",
    "localstorage-slim": "^2.2.0",
    "nx": "14.5.4",
    "prettier": "2.7.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "prettier-plugin-prisma": "^3.8.0",
    "ts-jest": "27.1.4",
    "ts-node": "10.8.1",
    "tslib": "^2.3.0",
    "typescript": "4.7.3",
    "zone.js": "~0.11.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "importSort": {
    ".js, .ts": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
