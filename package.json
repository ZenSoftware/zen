    "helmet": "^6.0.0",
    "nodemailer": "^6.7.7",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "prisma": "~3.15.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.5.0"
