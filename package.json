    "concurrently": "^5.3.0",
    "del": "^5.1.0",
    "eslint": "6.8.0",
    "graphql-tools": "^6.0.18",
    "gulp": "^4.0.2",
    "gulpclass": "^0.2.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "26.2.2",
