{
  "name": "zen",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nx serve portal",
    "start:api": "nx serve api",
    "start:e2e": "nx e2e portal-e2e --watch",
    "frontend:gen": "graphql-codegen -c apollo-angular.yml && graphql-codegen -c apollo-possible-types.yml",
    "api:gen": "prisma generate && ts-node generate.ts",
    "prisma:migrate": "prisma migrate dev && ts-node generate.ts",
    "prisma:create-only": "prisma migrate dev --create-only",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prod:apps": "nx build portal --prod",
    "prod:api": "nx build api --prod && npm run dockerize:api",
    "dockerize:api": "ts-node tools/create-deploy-package.ts && docker build -f deploy/api/Dockerfile -t zen-api .",
    "docker:run-api": "docker run -d --name zen-api -p 7080:7080 zen-api",
    "docs": "ng compodoc api",
    "test": "nx run-many --all --target=test --parallel",
    "lint": "nx run-many --all --target=lint --parallel",
    "format": "nx format:write --all",
    "build": "nx build portal",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@casl/ability": "^6.0.0",
    "@casl/prisma": "^1.0.4",
    "@graphql-tools/schema": "^9.0.1",
    "@nestjs-modules/mailer": "^1.8.1",
