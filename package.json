{
  "name": "zen",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "main": "apps/portal/src/electron.js",
  "scripts": {
    "start": "nx serve portal",
    "start:electron": "ng build --project portal --configuration=development --outputHashing=all --base-href=./ && electron .",
    "start:api": "nx serve api",
    "start:e2e": "nx e2e portal-e2e --watch",
    "frontend:gen": "graphql-codegen -c apollo-angular.yml && graphql-codegen -c apollo-possible-types.yml",
    "api:gen": "prisma generate --schema apps/api/schema.prisma && ts-node generate.ts",
    "prisma:migrate": "prisma migrate dev --schema apps/api/schema.prisma && ts-node generate.ts",
    "prisma:create-only": "prisma migrate dev --schema apps/api/schema.prisma --create-only",
    "prisma:deploy": "prisma migrate deploy --schema apps/api/schema.prisma",
    "prisma:studio": "prisma studio --schema apps/api/schema.prisma",
    "prod:apps": "nx build portal --prod",
    "prod:api": "nx build api --prod && npm run dockerize:api",
    "dockerize:api": "ts-node tools/create-deploy-package.ts && docker build -f deploy/api/Dockerfile -t zen-api .",
    "docker:run-api": "docker run -d --name zen-api -p 7080:7080 zen-api",
    "docs": "ng compodoc api",
    "test": "nx run-many --all --target=test --parallel",
    "lint": "nx run-many --all --target=lint --parallel",
    "format": "nx format:write --all",
    "build": "nx build portal",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main"
  },
  "dependencies": {
    "@casl/ability": "~6.3.0",
    "@casl/prisma": "~1.3.0",
    "@graphql-tools/schema": "^9.0.1",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/apollo": "^10.0.4",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.0.4",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/throttler": "^3.0.0",
    "@paljs/plugins": "5.1.0",
    "@prisma/client": "~4.7.0",
    "apollo-server-express": "~3.11.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "date-fns": "^2.17.0",
    "graphql": "~16.6.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-upload": "^15.0.2",
    "helmet": "^6.0.0",
    "nodemailer": "^6.7.7",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "prisma": "~4.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.5.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^15.0.0-beta.0",
    "@angular-devkit/build-angular": "~15.0.3",
    "@angular-eslint/eslint-plugin": "15.1.0",
    "@angular-eslint/eslint-plugin-template": "15.1.0",
    "@angular-eslint/template-parser": "15.1.0",
    "@angular/animations": "~15.0.3",
    "@angular/cdk": "~15.0.1",
    "@angular/cli": "~15.0.3",
    "@angular/common": "~15.0.3",
    "@angular/compiler": "~15.0.3",
    "@angular/compiler-cli": "~15.0.3",
    "@angular/core": "~15.0.3",
    "@angular/forms": "~15.0.3",
    "@angular/language-service": "~15.0.3",
    "@angular/localize": "~15.0.3",
    "@angular/material": "~15.0.1",
    "@angular/platform-browser": "~15.0.3",
    "@angular/platform-browser-dynamic": "~15.0.3",
    "@angular/router": "~15.0.3",
    "@apollo/client": "~3.7.1",
    "@casl/angular": "~8.1.0",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/fragment-matcher": "^3.2.0",
    "@graphql-codegen/typescript": "^2.4.0",
    "@graphql-codegen/typescript-apollo-angular": "^3.3.0",
    "@graphql-codegen/typescript-operations": "^2.2.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/angular": "15.3.0",
    "@nrwl/cli": "15.3.0",
    "@nrwl/cypress": "15.3.0",
    "@nrwl/eslint-plugin-nx": "15.3.0",
    "@nrwl/jest": "15.3.0",
    "@nrwl/linter": "15.3.0",
    "@nrwl/nest": "15.3.0",
    "@nrwl/node": "15.3.0",
    "@nrwl/workspace": "15.3.0",
    "@paljs/cli": "5.1.0",
    "@types/apollo-upload-client": "^17.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/graphql-upload": "^8.0.11",
    "@types/jest": "28.1.8",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^18.7.16",
    "@types/passport": "^1.0.6",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "apollo-angular": "^4.0.0",
    "apollo-upload-client": "^17.0.0",
    "bootstrap": "~5.2.0",
    "bootswatch": "~5.2.0",
    "codelyzer": "^6.0.1",
    "concurrently": "^7.0.0",
    "cypress": "^11.0.0",
    "cz-customizable": "^7.0.0",
    "dotenv": "^16.0.0",
    "electron": "^22.0.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "import-sort-style-module": "^6.0.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "12.2.3",
    "localstorage-slim": "^2.2.0",
    "lodash-es": "^4.17.20",
    "nx": "15.3.0",
    "prettier": "^2.6.2",
    "prettier-plugin-import-sort": "^0.0.7",
    "prettier-plugin-prisma": "^4.2.0",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~4.8.4",
    "zone.js": "~0.11.4"
  },
  "overrides": {
    "@casl/angular": {
      "@angular/core": "$@angular/core"
    },
    "apollo-angular": {
      "@angular/core": "$@angular/core"
    },
    "@paljs/plugins": {
      "graphql": "$graphql"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "importSort": {
    ".js, .ts": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
