{
  "name": "zen",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nx serve portal --host=127.0.0.1",
    "start:api": "nx serve api",
    "start:e2e": "nx e2e portal-e2e --watch",
    "frontend:gen": "graphql-codegen -c apollo-angular.yml && graphql-codegen -c apollo-possible-types.yml",
    "api:gen": "prisma generate --schema apps/api/schema.prisma && ts-node generate.ts",
    "prisma:migrate": "prisma migrate dev --schema apps/api/schema.prisma && ts-node generate.ts",
    "prisma:deploy": "prisma migrate deploy --schema apps/api/schema.prisma",
    "prisma:studio": "prisma studio --schema apps/api/schema.prisma",
    "prod:portal": "nx build portal --prod",
    "prod:api": "nx build api --prod && npm run docker:build:api",
    "docker:build:api": "ts-node tools/create-deploy-package.ts && docker build -f deploy/api/Dockerfile -t zen-api .",
    "docker:run:api": "docker run -d --name zen-api -p 7081:7080 zen-api",
    "doc:api": "nx run api:compodoc",
    "test": "nx run-many --all --target=test --parallel",
    "lint": "nx run-many --all --target=lint --parallel",
    "format": "nx format:write --all",
    "postinstall": "node ./decorate-angular-cli.js"
  },
  "dependencies": {
    "@casl/ability": "~6.3.3",
    "@casl/prisma": "~1.3.3",
    "@graphql-tools/merge": "^8.3.14",
