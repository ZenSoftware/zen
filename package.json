    "@angular/platform-browser-dynamic": "^10.0.0",
    "@angular/router": "^10.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/angular": "10.1.0",
    "@nrwl/jest": "10.1.0",
    "@nrwl/nest": "10.1.0",
    "@nrwl/node": "10.1.0",
    "@nrwl/workspace": "10.1.0",
